		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^10.2.3",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.1.0",
		"playwright": "^1.58.0",
		"prettier": "^3.8.1",
